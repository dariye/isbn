{
  "version": 2,
  "alias": "isbn",
  "functions": {
    "api/graphql/index.js": {
      "maxDuration": 60
    },
    "api/rest/[isbn].js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    { "source": "/graphql", "destination": "/api/graphql/index.js" },
    {
      "source": "/find/:path((?<isbn>[^/]*))",
      "destination": "/api/rest/[isbn].js"
    },
    {
      "source": "/",
      "destination": "/_site/index.html"
    }
  ]
}
